/* Eric Meyer's CSS Reset Reloaded */html, body, div, span, applet, object, iframe,h1, h2, h3, h4, h5, h6, p, blockquote, pre,a, abbr, acronym, address, big, cite, code,del, dfn, em, font, img, ins, kbd, q, s, samp,small, strike, strong, sub, sup, tt, var,dl, dt, dd, ol, ul, li,fieldset, form, label, legend,table, caption, tbody, tfoot, thead, tr, th, td {margin: 0;padding: 0;border: 0;outline: 0;font-weight: inherit;font-style: inherit;font-size: 100%;font-family: inherit;vertical-align: baseline;}:focus {outline: 0;}body {line-height: 1;}ol, ul {list-style: none;}table {border-collapse: separate;border-spacing: 0;}caption, th, td {text-align: left;font-weight: normal;}blockquote:before, blockquote:after,q:before, q:after {content: "";}blockquote, q {quotes: "" "";}/* My Styles */body {background-color:#4c4c4c;background-image: url(img/bg.png);background-repeat:repeat-x;background-position:left top;font-size:62.5%;font-family:Georgia, "Times New Roman", Times, serif;}a {text-decoration:none;}strong {font-weight:bold;}em {font-style:italic;}#wrapper {margin:0 auto;width:96em;padding-bottom:2em;padding-top:17.6em;position:relative;}#header {margin-top:2.5em;margin-bottom:4em;position:absolute;top:0;width:100%;}#header .holder {margin:0 auto;width:17.15em;}#slogan {background-color:#000;opacity:0.4;font-size:5.6em;text-transform:uppercase;color:#cacaca;display:block;font-family:"Helvetica Neue", Helvetica, Arial, sans-serif;position:relative;z-index:3;}a#slogan:hover {color:#DBAFAF;opacity:0.6;}a#slogan:active {opacity:1.0;}#nav {background-color:#960000;opacity:0.5;margin-top:2em;position:relative;z-index:0;}#nav ul {overflow:auto;height:100%;margin:0 auto;width:96em;}#nav li {float:left;list-style-type:none;font-size:1.6em;font-family:Helvetica, Arial, sans-serif}#nav li a {padding:0.6em;color:#da6363;display:block;margin-right:1.2em;}#nav li a:hover {background-color:#da6363;color:#fff;}#nav li a:active {background-color:#da8282;}#clock {background-image:url("img/clock-face.png");height:32.3em;position:absolute;right:-9.5em;top:0;width:63em;z-index:1;}#clock .hand {background-image:url("img/clock-hand.gif");background-repeat:no-repeat;height:30.2em;position:absolute;right:17em;top:-16em;width:30em;-webkit-animation-name: clockhand;-webkit-animation-iteration-count: infinite;-webkit-animation-timing-function: linear;-webkit-animation-duration: 60s;}#content,#gallery {float:left;}h1 {font-size:2.9em;line-height:110%;font-family:"Helvetica Neue", Helvetica, Arial, sans-serif;width:17em;}h1,h1 a {color:#bdbdbd;}h1 a:hover {color:#DBAFAF;}h1 a:active {color:#da8282;}#content {background-color:#6D6D69;border-bottom:0.6em solid #5F5F5D;font-family:Helvetica,Arial,sans-serif;margin-bottom:2em;margin-right:2em;margin-top:1.5em;padding:1.6em 1.8em 1.5em;position:relative;width:44em;z-index:2;}#content p {color:#d8d8d8;font-size:1.3em;line-height:145%;margin-bottom:0.8em;}#content a {color:#dbafaf;}#content a:hover {border-bottom:0.1em solid #dbafaf;}#content a:active {color:#da8282;border-bottom:0.1em solid #da8282;}#content .date {text-transform:uppercase;text-align:right;display:block;font-weight:bold;font-family:Helvetica, Arial, sans-serif;color:#41413e;text-shadow:0.1em 0.1em 0 #868683;letter-spacing:0.22em;margin-bottom:1.5em;}#content .caption {float:left;padding:0.6em 0.6em 0;background-color:#4d4d4d;font-family:Georgia, "Times New Roman", Times, serif;margin-right:1.5em;}#content .caption p {font-size:1.2em;font-style:italic;margin-bottom:0;padding:0.75em 0;text-align:center;}#content .more {background-image:url("img/keep-reading-button.png");background-position:right center;background-repeat:no-repeat;border-bottom:0 none;display:block;letter-spacing:0.22em;padding:1.5em 1.3em;text-align:right;text-transform:uppercase;}#content .more:hover {border-bottom:0 none;}#content .more:active {padding:1.6em 1.3em 1.4em;border-bottom:0 none;}#content #corner {border-right:2em solid #3C3C3A;border-top:2em solid #4b4b4b;left:0;position:absolute;top:0;width:0;}#gallery {padding-top:9em;width:46em;}#gallery li {float:left;width:14em;margin-right:2em;}#gallery li.last {margin-right:0;}#gallery li a {text-align:center;display:block;text-transform:uppercase;color:#bdbdbd;font-size:1em;opacity:0.65;}#gallery li a:hover {opacity:1.0;}#gallery li a img {padding:1em;background-color:#3c3c3c;border-top:0.2em solid #505050;border-left:0.2em solid #505050;}#gallery li a:hover img {background-color:#DBAFAF;}#gallery li a:active img {background-color:#da8282;}#gallery li a cite {text-transform:none;font-style:italic;text-decoration:underline;display:block;font-size:1.2em;padding-top:0.6em;padding-bottom:1.2em;}#gallery li a:hover cite {color:#DBAFAF;}#gallery li a:active cite {color:#da8282;}#gallery li p {font-size:1.1em;font-family:Helvetica, Arial, sans-serif;color:#bdbdbd;line-height:145%;}#callouts {clear:left;width:96em;}#callouts li {float:left;margin-right:2em;}#callouts li.last {margin-right:0;}#callouts li a {display:block;height:14.4em;width:29.8em;text-indent:-999em;background-image:url('img/callouts-sprite.jpg');background-position:top center;background-repeat:no-repeat;border-top:0.2em solid #5b5b57;border-left:0.2em solid #5b5b57;border-right:0.2em solid #323230;border-bottom:0.2em solid #323230;}#callouts li a:hover {border-color:#DBAFAF;}#callouts li a:active {border-color:#da8282;}#callouts li.first a {background-position:top left;}#callouts li.last a {background-position:top right;}#footer {background-color:#960000;clear:both;overflow:auto;height:100%;padding:0.5em 0;}#footer .holder {width:96em;margin:0 auto;}#footer #copyright{font-family:Helvetica, Arial, sans-serif;;font-size:1.4em;font-weight:bold;color:#da6363;width:45%;float:left;}#footer #quote {width:45%;float:right;text-align:right;font-size:1.1em;letter-spacing:0.44em;text-transform:uppercase;color:#da6363;}/*Special Sauce*/@-webkit-keyframes clockhand {	from {	-webkit-transform: rotate(0deg);	}		to {	-webkit-transform: rotate(360deg);	}}